            <div class="clearfix"></div>
            <!-- start form for validation -->
            <form method="post" action="register_employee" id="demo-form" data-parsley-validate>
                <div>
                    <label for="empid">Employee ID: *</label>
                    <input id="empid" name="empid" type="text" class="form-control" placeholder="Employee ID" required="" />
                </div>
                <div>
                    <label for="firstname">First Name: *</label>
                    <input id="firstname" name="firstname" type="text" class="form-control" placeholder="Name" required="" />
                </div>
                <div>
                    <label for="lastname">Last Name: *</label>
                    <input id="lastname" name="lastname" type="text" class="form-control" placeholder="Surname" required="" />
                </div>
                <div class="form-group">
                    <label for="rolelist">Role: *</label>
                    <select class="form-control" id="rolelist">
                        <option>Employee</option>
                        <option>Manager</option>
                        <option>Admin</option>
                    </select>
                </div>
                <div>
                    <label for="email">Email: *</label>
                    <input id="email" name="email" type="email" class="form-control" placeholder="Email" required="" />
                </div>
                <!--<div>-->
                    <!--<label for="password">Password: *</label>-->
                    <!--<input id="password" name="password" type="password" class="form-control" placeholder="Password" required="" />-->
                <!--</div>-->
                <!--<div>-->
                    <!--<label for="confirmpassword">Confirm Password: *</label>-->
                    <!--<input id="confirmpassword" name="confirmpassword" type="password" class="form-control" placeholder="Confirm Password" required="" />-->
                <!--</div>-->

                <div>
                    <label for="emp_length">Length of Employment (In years): *</label>
                    <input id="emp_length" name="emp_length" type="number" minlength="0" class="form-control" placeholder="1" required="" />
                </div>

                <div class="form-group">
                    <label class="control-label">Skill Tags</label>
                    <input id="tags_1" name="skills" type="text" class="tags form-control" value="MS Office, Accounting Skills, Auditor" data-parsley-trigger="change" required  />
                </div>

                <div class="form-group">
                    <label  class="control-label">Past Projects</label>
                    <input id="" name="pastprojects" type="text" class="tags form-control" value="MS Office, Accounting Skills, Auditor" data-parsley-trigger="change" required  />
                </div>

                <div>
                    <input class="btn btn-default submit" type="submit" value="Submit">
                </div>

                <div class="clearfix"></div>
            </form>

<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <!-- Sidebar -->
        <%- include('sidebar'); %>

        <!-- top navigation -->
        <%- include('top_nav'); %>
        <!-- /top navigation -->

        <!-- page content -->
        <div class="right_col" role="main">
            <!-- top tiles -->
            <div class="row tile_count">
                <div class="col-md-3 col-sm-3 col-xs-6 tile_stats_count">
                    <span class="count_top"><i class="fa fa-tasks"></i> Completed Projects</span>
                    <div class="count kpmg-blue" id="completed_projects"></div>
                </div>
                <div class="col-md-3 col-sm-3 col-xs-6 tile_stats_count">
                    <span class="count_top"><i class="fa fa-tasks"></i> All Ongoing Projects</span>
                    <div class="count kpmg-blue" id="ongoing_projects"></div>
                </div>
                <div class="col-md-3 col-sm-3 col-xs-6 tile_stats_count">
                    <span class="count_top"><i class="fa fa-user"></i> Total Employees</span>
                    <div class="count kpmg-blue" id="total_emp"></div>
                </div>
                <div class="col-md-3 col-sm-3 col-xs-6 tile_stats_count">
                    <span class="count_top"><i class="fa fa-user"></i> Total Managers</span>
                    <div class="count kpmg-blue" id="total_man"></div>
                </div>
            </div>
            <!-- /top tiles -->
            <div id="analytics_graphs">

            </div>
            <br/>
        </div>
        <!-- /page content -->
        <script>
            $(document).ready(function() {
                $.get("get_all_past_projects",{},function (data,status) {

                    $("#completed_projects").empty();
                    $("#completed_projects").append(data[0]);
                    $("#ongoing_projects").empty();
                    $("#ongoing_projects").append(data[1]);
                    $("#total_emp").empty();
                    $("#total_emp").append(data[2]);
                    $("#total_man").empty();
                    $("#total_man").append(data[3]);
                });
            });
        </script>
        <!-- footer content -->
        <footer>
            <div class="pull-right">
                Gentelella - Bootstrap Admin Template by <a href="https://colorlib.com">Colorlib</a>
            </div>
            <div class="clearfix"></div>
        </footer>
        <!-- /footer content -->
    </div>
</div>



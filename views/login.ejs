<body class="login">
<div>
    <a class="hiddenanchor" id="signup"></a>
    <a class="hiddenanchor" id="signin"></a>

    <div class="login_wrapper">
        <div class="animate form login_form">
            <section class="login_content">
                <form >
                    <h1>Login</h1>
                    <div>
                        <input type="text" maxlength="8" class="form-control" id="username" name="username" placeholder="Username" required/>
                    </div>
                    <div>
                        <input type="password" class="form-control" id="password" name="password" placeholder="Password" required/>
                    </div>
                    <div>
                        <button id="login_btn" class="btn btn-default submit" type="button" value="Submit">Login</button>
                        <!--<a class="reset_pass" href="#">Lost your password?</a>-->
                        <!--Maybe we can add a lost your password part in later on -->
                    </div>

                    <div class="clearfix"></div>

                    <div class="separator">
                        <div class="clearfix"></div>
                        <br/>

                        <div>
                            <img src="images/logo/logo-black.png" width="145px" height="90px">
                            <br/>
                            <br/>
                            <p>Â©2017 All Rights Reserved.</p>
                        </div>
                    </div>
                </form>
            </section>
        </div>
    </div>
</div>

</body>

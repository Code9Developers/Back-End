<body class="nav-md">
<div class="container body">
    <div class="main_container">
        <!-- Sidebar -->
        <%- include('sidebar'); %>

        <!-- top navigation -->
        <%- include('top_nav'); %>
        <!-- /top navigation -->

        <!-- page content -->
        <div class="right_col" role="main">
            <div class="title_left">
                <h3>Project Review
                    <small> PROJECT NAME</small>
                </h3>
                <h4>
                    Manger Name
                </h4>
            </div>

            <hr/>

            <div class="clearfix"></div>

            <!-- start form for validation -->
            <div class="title">
                <h5>Overall Project Review</h5>
            </div>
            <form id="demo-form" data-parsley-validate >
                <div class="row">
                    <div class="col-md-offset-1 col-md-4">
                        <label for="overallProjRating">Overall project rating: *</label>
                        <input type="text" id="overallProjRating" value="" name="overallProjRating"/>
                    </div>

                </div>
                <hr/>
                <div class="title">
                    <h5>Employee Review</h5>
                </div>

                <!--ONE EMPLOYEEE START-->
                <div id="all_user_info">

                </div>


                <div class="col-md-12">
                    <input id="submit_review" class="btn btn-dark docs-tooltip" data-toggle="tooltip" title="Submit Review" type="button" value="Complete Review">
                </div>
            </form>
            <!-- end form for validations -->
        </div>
        <!-- /page content -->

        <!-- footer content -->
        <footer>
            <div class="pull-right">
                Gentelella - Bootstrap Admin Template by <a href="https://colorlib.com">Colorlib</a>
            </div>
            <div class="clearfix"></div>
        </footer>
        <!-- /footer content -->
    </div>
</div>

